# QlikSense Server Configuration
# Replace with your actual QlikSense server URL
QLIK_SERVER=https://your-qlik-server.com

# QlikSense Authentication
# Username for QlikSense authentication
QLIK_USER=your-username

# Password for QlikSense authentication (consider using more secure methods in production)
QLIK_PASSWORD=your-password

# Alternative: API Key authentication (if supported by your QlikSense setup)
# QLIK_API_KEY=your-api-key

# QlikSense Application Configuration
# Default app ID to work with (optional)
APP_ID=your-default-app-id

# MCP Server Configuration
# Port for the MCP server to listen on
MCP_PORT=8080

# Host for the MCP server
MCP_HOST=localhost

# Logging Configuration
# Log level: DEBUG, INFO, WARNING, ERROR, CRITICAL
LOG_LEVEL=INFO

# Log file path (optional, logs to console if not specified)
# LOG_FILE=logs/qlik-mcp-server.log

# QlikSense API Configuration
# Request timeout in seconds
REQUEST_TIMEOUT=30

# Maximum number of retries for failed requests
MAX_RETRIES=3

# Connection pool size
CONNECTION_POOL_SIZE=10

# SSL verification (set to false only for development with self-signed certificates)
SSL_VERIFY=true